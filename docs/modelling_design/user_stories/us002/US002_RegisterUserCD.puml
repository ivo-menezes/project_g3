@startuml

class RegisterAccountUI {}

class RegisterAccountController {

registerAccount(name, email, phone)
registerAccount(name, email, phone, photo)
}

class Services {
accountList : AccountList
profileList : ProfileList
registerAccount(name, email, phone)
registerAccount(name, email, phone, photo)
}

class ProfileList {
}

class AccountList {
save(newAccount)
}

class Profile {
name: String
}

class Account {
name : String
email : String
phone : String
photo : Photo
