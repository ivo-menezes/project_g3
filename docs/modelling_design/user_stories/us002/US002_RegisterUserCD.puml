Services "                               1" o-- "        1" ProfileList
Services ..> Profile
Services "        1" o-- "             1" AccountList
Services ..> Account
Account ..> Profile
AccountList "  1" *-- "   *" Account : has
ProfileList "1" *-- "                  *" Profile : has

@enduml