@startuml

class ListAccountsUI {}

class ListAccountsController{
'services : Services
listAccounts()
}

class Services {
'accountList : AccountList
listAccounts()
}

class Account {
profile : Profile
name : String
email : String
phone : String
photo : Photo
isActive : boolean
}

class AccountList {
'accounts: List<Account>
getAccountAtIndex(index)
}

class AccountDTO {
email : String
isActive : boolean
}

AccountList "1" *-- "*" Account : has
Services ..> Account
Services "1" o-- "1" AccountList
ListAccountsController "1" o-- "1" Services
ListAccountsUI "1" o-- "1" ListAccountsController
Services ..> AccountDTO
AccountDTO ..> Account

@enduml